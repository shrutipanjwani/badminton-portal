\n\r\n    const styles = {\r\n      center: {\r\n        textAlign: \"center\",\r\n      },\r\n    };\r\n\r\n    const submitForm = async (e) => {\r\n      const params = {\r\n        params: {\r\n          \"email\": email.toLowerCase(),\r\n        },\r\n      };\r\n      var res;\r\n      e.preventDefault();\r\n\r\n      try {\r\n        await axios.get(\"/users/resetpasswordmail\", params);\r\n        //console.log(res);\r\n        handleClickOpenEmailRequest(true)\r\n      } catch (err) {\r\n        if (err.response.data.errors[0].msg) {\r\n          setAlert(err.response.data.errors[0].msg, \"danger\");\r\n        }\r\n        else {\r\n          setAlert(\"Something went Wrong, Please retry\");\r\n        }\r\n        //console.log(err.response.data.errors[0].msg)\r\n      }\r\n    }\r\n\r\n    if (isAuthenticated) {\r\n     // console.log(isAdmin)\r\n      if(isAdmin){\r\n        //console.log(isAdmin)\r\n       return <Redirect to='/permission' replace />;\r\n      }else{\r\n        //console.log(isAdmin)\r\n       return <Redirect to='/calendar' replace />;\r\n      }\r\n    }\r\n\r\n    \r\n    return <Fragment>\r\n        <section className=\"container\">\r\n          <h1 className=\"large text-primary\">Sign In</h1>\r\n          <form className=\"form\" onSubmit={(e) => onSubmit(e)}>\r\n            <div className=\"form-group\">\r\n              <input\r\n                type=\"email\"\r\n                placeholder=\"Email Address\"\r\n                name=\"email\"\r\n                value={email.toLowerCase()}\r\n                onChange={(e) => onChange(e)}\r\n                required\r\n              />\r\n            </div>\r\n            <div className=\"form-group\">\r\n              <input\r\n                type=\"password\"\r\n                placeholder=\"Password\"\r\n                name=\"password\"\r\n                minLength=\"6\"\r\n                value={password}\r\n                onChange={(e) => onChange(e)}\r\n                required\r\n              />\r\n            </div>\r\n            <Alert />\r\n            <p onClick={handleClickOpen} style={{ cursor: 'pointer'}}>Forgot Password?</p>\r\n            <br />\r\n            <br />\r\n            <input type=\"submit\" className=\"btn btn-primary\" value=\"Login\" />\r\n          </form>\r\n            <Dialog\r\n              open={open}\r\n              onClose={handleClose}\r\n              aria-labelledby=\"resetpassword\"\r\n            >\r\n              <DialogContent id=\"resetpassword\">\r\n                  <DialogContentText style={styles.center}>\r\n                    Enter Your Registered Email to Reset Password\r\n                    <form onSubmit={(e) => submitForm(e)}>\r\n                      <div>\r\n                        <input\r\n                          type=\"email\"\r\n                          placeholder=\"Email Address\"\r\n                          name=\"email\"\r\n                          value={email.toLowerCase()}\r\n                          onChange={(e) => onChange(e)}\r\n                          required\r\n                          style={{ padding: '10px', width: '250px', marginTop: '20px'}}\r\n                        />\r\n                      </div>\r\n                      <br />\r\n                      <br />\r\n                      <input type=\"submit\" className=\"btn btn-primary\" value=\"Send Reset Password Email\"  onClick={handleClose}/>\r\n                    </form>\r\n                  </DialogContentText>\r\n              </DialogContent>\r\n            </Dialog>\r\n            <Dialog\r\n              open={start}\r\n              onClose={handleCloseEmailRequest}\r\n              aria-labelledby=\"emailrequest\"\r\n            >\r\n              <DialogContent id=\"emailrequest\">\r\n                  <DialogContentText style={styles.center}>\r\n                    <p>Email to Reset Password has been sent to {email.toLowerCase()}.</p>\r\n                    <p>Please Check 