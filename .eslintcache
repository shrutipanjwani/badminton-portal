rd\"\r\n              name=\"password\"\r\n              minLength=\"6\"\r\n              value={password}\r\n              onChange={(e) => onChange(e)}\r\n              required\r\n            />\r\n          </div>\r\n          <div className=\"form-group\">\r\n            <input\r\n              type=\"password\"\r\n              placeholder=\"Confirm Password\"\r\n              name=\"confirmPassword\"\r\n              minLength=\"6\"\r\n              value={confirmPassword}\r\n              onChange={(e) => onChange(e)}\r\n              required\r\n            />\r\n          </div>\r\n          <div className=\"form-group\">\r\n            <label for=\"level\" style={{textAlign: \"left\"}}>Choose a Level:</label>\r\n            <select\r\n                id=\"level\"\r\n                name=\"level\"\r\n                value={level}\r\n                onChange={(e) => onChange(e)}\r\n                required\r\n            >\r\n                <option value=\"Basic\" selected>Basic</option>\r\n                <option value=\"Low Intermediate\">Low Intermediate</option>\r\n                <option value=\"Intermediate\">Intermediate</option>\r\n                <option value=\"Advanced\">Advanced</option>\r\n            </select>\r\n          </div>\r\n          <div className={classes.dflex}>\r\n            <Autocomplete\r\n              id=\"country-select-demo\"\r\n              style={{ width: 120, margin: \"auto\" }}\r\n              options={countries}\r\n              defaultValue={countries[12]}\r\n              classes={{\r\n                option: classes.option,\r\n              }}\r\n              autoHighlight\r\n              getOptionLabel={(option) => option.phone}\r\n              renderOption={(option) => (\r\n                <React.Fragment>\r\n                  <span>{countryToFlag(option.code)}</span>({option.code}) +\r\n                  {option.phone}\r\n                </React.Fragment>\r\n              )}\r\n              renderInput={(p